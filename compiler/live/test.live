cursorW = 0
cursorH = 0
maxWidth = 80
maxHeight = 25

halt = ->
  asm 'halt'

outb = (pos, char) ->
  asm 'outb', pos, char

strlen = ->
  b = 9
  return b

putchar = (char) ->
  pos = cursorH * maxWidth + cursorW
  outb pos, char
  cursorW++
  if cursorW is maxWidth
    cursorW = 0
    cursorH++

putstr = (str) ->
  while *str isnt 0
    putchar *str
    str++

putstr 'lol'

halt!
