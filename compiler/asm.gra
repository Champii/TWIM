S: Statement+

Statement: LabelDecl | Expression | "\n"

Expression: Opcode SpaceArg+ "\n"?

Opcode: Letter+

SpaceArg: " " Arg

Arg: Literal | LabelUse | Reg | Deref

Reg: "a" | "b" | "c" | "d" | "ip" | "cr"

LabelDecl: Label ":\n"

LabelUse: ":" Label

Label: Letter+

Literal: Char | Digit+

Char: "'" Letter "'"

Deref: "[" Arg "]"
